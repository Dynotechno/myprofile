{% extends 'base_profile.html.twig' %}

{% block title %}UserLanguage{% endblock %}

{% block container_profile %}
    <h1>UserLanguage</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ user_language.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ user_language.name }}</td>
            </tr>
            <tr>
                <th>Level</th>
                <td>{{ user_language.level }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('user_language_index') }}">back to list</a>

    <a href="{{ path('user_language_edit', {'id': user_language.id}) }}">edit</a>

    {{ include('user_language/_delete_form.html.twig') }}
{% endblock %}
