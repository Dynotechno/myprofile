{
  "name": "myProfile",
  "env": {
    "APP_ENV": {
      "description" : "eg. dev, test, qa or prod"
    },
    "NPM_CONFIG_PRODUCTION": {
      "description" : "eg. true or false"
    },
    "YARN_PRODUCTION": {
      "description" : "eg. true or false"
    },
    "MAILER_TRANSPORT": {
      "description" : "smtp",
      "value": "smtp"
    },
    "MAILER_HOST": {
      "description": "info your host 127.0.0.1"
    },
    "MAILER_USER": {
      "description": "report your user"
    },
    "MAILER_PASSWORD": {
      "description": "report your password"
    },
    "MAILER_TO": {
      "description": "report about your email for received eg: email_to@teste.com"
    },
    "MAILER_FROM": {
      "description": "report about your configured email email_from@teste.com"
    },
    "SENTRY_DSN": {
      "description": "Please set here SDK dsn sentry."
    },
    "TRANSLOADIT_KEY": {
      "description": "Transloadit key."
    },
    "TRANSLOADIT_SECRET": {
      "description": "Transloadit secret."
    },
    "TRANSLOADIT_TEMPLATE_ID_CURRICULUM": {
      "description": "Transloadit curriculum template id."
    },
    "TRANSLOADIT_TEMPLATE_ID_IMAGE_PROFILE": {
      "description": "Transloadit image profile template id."
    },
    "TRANSLOADIT_TEMPLATE_ID_IMAGE_BACKGROUND": {
      "description": "Transloadit image background template id."
    },
    "TRANSLOADIT_CREDENTIALS": {
      "description": "Transloadit credentials."
    },
    "TRANSLOADIT_DELIVERY": {
      "description": "Transloadit delivery flag 1 or 0."
    },
    "BUCKET_DNS": {
      "description": "Bucket dns."
    },
    "BUCKET_NAME": {
      "description": "Bucket name."
    },
    "CDN_DNS": {
      "description": "CDN host."
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "9.5"
      }
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/php"
    },
    {
      "url": "heroku/nodejs"
    }
  ]
}